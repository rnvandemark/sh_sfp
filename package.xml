<?xml version="1.0"?>
<package format="2">
	<name>smart_home_scc</name>
	<version>0.0.1</version>
	<description>The screen color coordinator controllers for the smart home.</description>

	<maintainer email="rnvandemark@gmail.com">Nick Vandemark</maintainer>
	<license>Do_Whatever_You_Want</license>

